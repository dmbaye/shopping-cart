    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "slim/slim": "^3.12",
        "slim/flash": "^0.4.0",
        "slim/csrf": "^0.8.3",
        "slim/twig-view": "^2.4",
        "illuminate/database": "^5.7",
        "monolog/monolog": "^1.24",
        "respect/validation": "^1.1",
        "vlucas/phpdotenv": "^3.3",
        "php-di/slim-bridge": "^2.0",
        "braintree/braintree_php": "^3.39"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "behat/behat": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
}
